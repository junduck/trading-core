{
  "name": "@junduck/trading-core",
  "version": "0.0.1",
  "description": "Core data structures and utils for spot market trading",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "trading",
    "backtest",
    "portfolio",
    "position",
    "spot-market",
    "bookkeeping",
    "finance",
    "investment"
  ],
  "author": "Jun",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junduck/trading-core.git"
  },
  "bugs": {
    "url": "https://github.com/junduck/trading-core/issues"
  },
  "homepage": "https://github.com/junduck/trading-core#readme",
  "packageManager": "pnpm@10.20.0",
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  }
}
